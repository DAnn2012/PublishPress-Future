{
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": {
            "*": "dist"
        },
        "platform-check": false,
        "autoloader-suffix": "PublishPressFuture"
    },
    "require": {
        "php": ">=7.2.5",
        "ext-json": "*",
        "woocommerce/action-scheduler": "^3.5",
        "psr/container": "1.0.0",
        "pimple/pimple": "3.2.3",
        "publishpress/instance-protection": "^1.0",
        "publishpress/wordpress-reviews": "^1.1",
        "publishpress/wordpress-banners": "^1.2",
        "publishpress/wordpress-version-notices": "1.1.4"
    }
}
